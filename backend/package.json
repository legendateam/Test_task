    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start:dev": "rimraf dist && npx tsc-watch --onSuccess \"ts-node ./src/app.ts\"",
        "lint": "npx eslint --cache --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "prettier:write": "npx prettier --write",
        "format": "npm run prettier:write src",
        "format:all": "npm run prettier:write .",
        "format:conflict": "npx eslint-config-prettier src/**",
        "migration:create": "typeorm migration:create",
        "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/configs/data-source.ts",
        "migration:run": "npx typeorm-ts-node-commonjs migration:run -- -d ./src/configs/data-source.ts",
        "migration:revert": "typeorm migration:revert -- -d ./src/configs/data-source.ts"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.9.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.12"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^18.13.0",
        "@types/prettier": "^2.7.2",
        "@types/reflect-metadata": "^0.1.0",
        "@types/tsc-watch": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.33.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
